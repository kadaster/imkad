      <xs:annotation>
         <xs:documentation source="http://www.imvertor.org/schema-info/technical-documentation">JaarlijksBedrag is een samengesteld datatype waamee aangegeven kan worden wat de hoogte is van een erfpachtcanon en of dat bedrag betrekking heeft op meerdere onroerende zaken. </xs:documentation>
      </xs:annotation>
   </xs:element>
   <xs:complexType name="JaarlijksBedrag" abstract="false">
      <xs:sequence>
         <xs:element name="bedrag" type="t:Bedrag" minOccurs="1" maxOccurs="1">
            <xs:annotation>
               <xs:documentation source="http://www.imvertor.org/schema-info/technical-documentation">Bedrag is het bedrag in euros van het jaarlijks bedrag.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="betreftMeerOnroerendeZaken"
                     type="xs:boolean"
                     minOccurs="1"
                     maxOccurs="1">
            <xs:annotation>
               <xs:documentation source="http://www.imvertor.org/schema-info/technical-documentation">Betreft meer onroerende zaken is een indicatie waarmee wordt aangegeven dat het jaarlijks bedrag meerdere onroerende zaken betreft.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:element name="TypeBreuk" type="r:TypeBreuk" abstract="false">
